def main():    passes = 0    failures = 0    student = 1
    while student <= 10:        result = int(input("Enter result (1 = pass, 0 = fail): "))            

    if result == 1:            passes += 1        else:            failures += 1                student += 1
    print(f"Passed {passes}")    print(f"Failed {failures}")
    if passes > 8: 

   print("Bonus to others!")